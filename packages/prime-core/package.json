{
  "name": "@primecms/core",
  "version": "0.1.9",
  "description": "The core Prime CMS Engine",
  "author": "Birkir Gudjonsson <birkir.gudjonsson@gmail.com>",
  "homepage": "https://github.com/birkir/prime",
  "license": "MIT",
  "main": "lib/index.js",
  "bin": {
    "primecms": "./cli.js"
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib",
    "cli.js"
  ],
  "scripts": {
    "start": "node lib/index.js",
    "prepublishOnly": "npm run build",
    "build": "rm -rf ./lib && tsc -p .",
    "dev": "DEBUG=prime* ts-node src",
    "dev:debug": "DEBUG=* ts-node src",
    "db:init": "DEBUG=prime* ts-node src/migrations/init.ts",
    "db:seed": "DEBUG=prime* ts-node src/migrations/seed.ts",
    "db:up": "db-migrate --migrations-dir ./src/migrations up",
    "db:down": "db-migrate --migrations-dir ./src/migrations down",
    "rebuild-deps": "lerna run build --ignore @primecms/core --ignore @primecms/ui",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@primecms/field": "^0.1.3",
    "@primecms/field-asset": "^0.1.7",
    "@primecms/field-boolean": "^0.1.3",
    "@primecms/field-datetime": "^0.1.3",
    "@primecms/field-document": "^0.1.5",
    "@primecms/field-group": "^0.1.7",
    "@primecms/field-slice": "^0.1.7",
    "@primecms/field-string": "^0.1.7",
    "acl": "0.4.11",
    "apollo-server-express": "2.2.0",
    "bcrypt": "3.0.2",
    "body-parser": "1.18.3",
    "connect-session-sequelize": "6.0.0",
    "cors": "2.8.5",
    "dataloader-sequelize": "1.7.5",
    "db-migrate": "0.11.4",
    "db-migrate-pg": "0.4.0",
    "debug": "4.1.0",
    "dotenv": "6.1.0",
    "express": "4.16.4",
    "express-session": "1.15.6",
    "faker": "4.1.0",
    "graphql": "14.0.2",
    "graphql-relay": "0.5.5",
    "graphql-sequelize": "9.0.4",
    "graphql-type-json": "0.2.1",
    "hashids": "1.2.2",
    "lodash": "4.17.11",
    "passport": "0.4.0",
    "passport-local": "1.0.0",
    "pg": "7.6.0",
    "rc": "1.2.8",
    "read-pkg": "4.0.1",
    "reflect-metadata": "0.1.12",
    "sequelize": "4.41.0",
    "sequelize-typescript": "0.6.7-beta.0",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@types/acl": "0.4.35",
    "@types/bcrypt": "3.0.0",
    "@types/db-migrate-base": "0.0.5",
    "@types/faker": "4.1.4",
    "@types/graphql": "14.0.3",
    "@types/lodash": "4.14.118",
    "@types/rc": "0.0.1",
    "ts-node": "7.0.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "330de5101b01a3604d708b7d5991943fb57db9ef"
}
